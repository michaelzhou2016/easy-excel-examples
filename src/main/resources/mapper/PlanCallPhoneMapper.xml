<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ai.guiji.easyexcel.examples.mapper.PlanCallPhoneMapper">
    <insert id="addPlanPhoneBatch">
        INSERT INTO plan_call_phone(phone, cust_name,cust_company)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.phone,jdbcType=VARCHAR}, #{item.custName,jdbcType=VARCHAR}, #{item.custCompany,jdbcType=VARCHAR})
        </foreach>
    </insert>
</mapper>